            \override Score.BarNumber #'transparent = ##t
            \time 2/4

            c4^\markup { \column {\bold {Allegro} \line {\bold {Parte 1} - Melodia } \line {\bold {a \hspace #1.0 1 \tiny \raise #0.5 "a"  voz }}}}
            

